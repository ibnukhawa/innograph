<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    	<record id="accounting_report_wiz_inherit" model="ir.ui.view">
            <field name="name">accounting.report.wiz.inherit</field>
            <field name="model">accounting.report</field>
            <field name="inherit_id" ref="account.accounting_report_view"/>
            <field name="arch" type="xml">
    	        <xpath expr="//form/group[1]" position="after">
					<group>
						<field name="wbs_ids"/>
						<field name="hide_detail"/>
					</group>
				</xpath>
            </field>
        </record>
    </data>
</odoo>